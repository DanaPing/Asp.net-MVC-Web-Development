@model Vidly.Models.Movie
@{
    ViewBag.Title = Model.Name;
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>@Model.Name</h2>
<ul>
    <li>Genre Type: @Model.Genre.GenreType</li>
    <li>Release Date: @Model.ReleaseDate</li>
    <li>Added Date: @Model.AddDate</li>
    <li>Number InStock: @Model.NumberInStock</li>
    
</ul>

