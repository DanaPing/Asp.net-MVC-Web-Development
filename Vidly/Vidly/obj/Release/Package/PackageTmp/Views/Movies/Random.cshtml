
@{
    ViewBag.Title = "Random";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<table class ="table table-bordered table-hover">
    <thead>
    <tr>
        <th>MovieName</th>
    </tr>
    </thead>
    <tbody>
    @foreach (var movie in Model)
    {   
        <tr>
            <td>@movie.Name</td>
        </tr>
    }
    </tbody>    
</table>


